{{#if resume.awards.length}}
<section id="awards">
	<h2>Awards</h2>
	{{#each resume.awards}}
	<div class="item">
		{{#if title}}
		<h3 class="title">
			{{title}}
		</h3>
		{{/if}}

		{{#if date}}
		<div class="date">
			{{date}}
		</div>
		{{/if}}

		{{#if awarder}}
		<div class="position">
			{{awarder}}
		</div>
		{{/if}}

		{{#if summary}}
		<div class="highlights">
			<p>{{summary}}</p>
		</div>
		{{/if}}
	</div>
	{{/each}}
</section>
{{/if}}
